<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shahr.salesmanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.WelcomeActivity" />
        <activity android:name=".register.RegisterActivity" />
        <activity android:name=".customer.CustomerActivity" />
        <activity android:name=".sales.SalesActivity" />
        <activity android:name=".statistics.StatisticsActivity" />
        <activity android:name=".products.ProductsActivity"></activity>
    </application>

</manifest>